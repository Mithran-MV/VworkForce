{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "AI contract drafting agent",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "_id": {
        "type": "string",
        "format": "uuid",
        "coerce": true
      },
      "contract_type": {
        "type": "object",
        "properties": {
          "$share": {
            "type": "string"
          }
        },
        "required": ["$share"]
      },
      "contract_description": {
        "type": "object",
        "properties": {
          "$share": {
            "type": "string"
          }
        },
        "required": ["$share"]
      },
      "contract_content": {
        "type": "object",
        "properties": {
          "$share": {
            "type": "string"
          }
        },
        "required": ["$share"]
      }
     
    },
    "required": ["_id", "contract_type", "contract_description", "contract_content"]
  }
}